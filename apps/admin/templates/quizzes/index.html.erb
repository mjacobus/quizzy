<h1>Quizzes</h1>

<table cellspacing='0'>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Private</th>
      <th>Created at</th>
      <th>Updated at</th>
    </tr>
  </thead>
  <tbody>
    <% quizzes.each do |quiz| %>
      <tr>
        <td> <%= quiz.title %> </td>
        <td> <%= quiz.description %> </td>
        <td> <%= quiz.private %> </td>
        <td> <%= quiz.created_at %> </td>
        <td> <%= quiz.updated_at %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

